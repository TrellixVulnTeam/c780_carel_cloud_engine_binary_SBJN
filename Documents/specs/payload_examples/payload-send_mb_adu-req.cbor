{                                  //TAG TYPE
                                   //    this is the request header common to all requests.
									     
    "LID": 1,                      //LID C_UINT16       integer line identifier       
    "SEQ": 10                      //SEQ C_UINT16       integer, ADU identifier. Useful to detect wrong order in 
                                   //                   message de-queueing. GW must return as-is in the response.
    "ADU": 010300010001AABB        //ADU C_BYTE[512]    string, Modbus request in hex format max. size id TBC.
        
}
