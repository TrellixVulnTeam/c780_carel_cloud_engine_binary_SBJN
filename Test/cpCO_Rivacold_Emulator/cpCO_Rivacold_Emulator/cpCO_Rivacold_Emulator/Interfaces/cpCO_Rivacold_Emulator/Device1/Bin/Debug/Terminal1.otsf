<?xml version="1.0" encoding="utf-8"?>
<FormatFields>
  <FormatField Name="CL_Move" Format="ib">
    <KeyBindings>
      <KeyBinding Key="UP" BindOn="Move_UP" />
      <KeyBinding Key="DOWN" BindOn="Move_DOWN" />
      <KeyBinding Key="ENTER" BindOn="SetCurrentValue" />
    </KeyBindings>
  </FormatField>
  <FormatField Name="CL_BOOL_SWAP" Format="iob">
    <KeyBindings>
      <KeyBinding Key="UP" BindOn="BoolSwap" />
      <KeyBinding Key="DOWN" BindOn="BoolSwap" />
      <KeyBinding Key="ENTER" BindOn="SetCurrentValue" />
    </KeyBindings>
  </FormatField>
  <FormatField Name="CL_INT" Format="io+5">
    <KeyBindings>
      <KeyBinding Key="UP" BindOn="Increment_WORD" />
      <KeyBinding Key="DOWN" BindOn="Decrement_WORD" />
      <KeyBinding Key="ENTER" BindOn="SetCurrentValue" />
    </KeyBindings>
  </FormatField>
</FormatFields>